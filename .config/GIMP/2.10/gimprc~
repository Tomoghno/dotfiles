# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(thumbnail-size large)
(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-raw-placeholder/file-raw-placeholder")
(check-update-timestamp 1608613087)
(last-release-timestamp 1601769600)
(monitor-xresolution 94)
(monitor-yresolution 95)
(toolbox-group-menu-mode hover)
(theme "System")
(icon-theme "Color")
(layer-new-name "Shine")
(path-import-path "/home/tomoghno/Downloads")
(selection-grow-radius 5)
(fill-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10))
(stroke-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10)
    (method line)
    (width 25)
    (unit pixels)
    (cap-style butt)
    (join-style miter)
    (miter-limit 10)
    (dash-offset 0)
    (dash-info 0)
    (emulate-brush-dynamics no))


